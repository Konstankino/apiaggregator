    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-json-pretty": "^2.0.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.5",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "^0.85.0",
    "webpack": "^4.29.5",
    "webpack-dev-server": "^3.1.14"
