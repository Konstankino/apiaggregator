    "enzyme-to-json": "^3.3.5",
    "jest": "^24.1.0",
    "jest-enzyme": "^7.0.1",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.8.3",
    "redux-mock-store": "^1.5.3",
    "webpack-cli": "^3.2.3"
